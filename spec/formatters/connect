store Test {
  state a : String = "a"
  state b : String = "b"
  state c : String = "c"
  state d : String = "d"
}

component A {
  connectTest exposing{a,b,c,d}

  fun render : Html {
    <div/>
  }
}
--------------------------------------------------------------------------------
store Test {
  state a : String = "a"
  state b : String = "b"
  state c : String = "c"
  state d : String = "d"
}

component A {
  connect Test exposing { a, b, c, d }

  fun render : Html {
    <div/>
  }
}
