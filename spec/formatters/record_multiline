type A {
  a : String,
  b : Bool
}

module Test {
  fun test : A {
    {a:"Hello this is a looong string so it will only fit in the next line.",b:true}
  }
}
--------------------------------------------------------------------------------
type A {
  a : String,
  b : Bool
}

module Test {
  fun test : A {
    {
      a: "Hello this is a looong string so it will only fit in the next line.",
      b: true
    }
  }
}
